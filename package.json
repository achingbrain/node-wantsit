{
  "name": "wantsit",
  "version": "2.1.3",
  "description": "Super lightweight dependency injection",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha",
    "test:coverage": "./node_modules/istanbul/lib/cli.js --include-all-sources cover ./node_modules/mocha/bin/_mocha",
    "test:coverage:verify": "./node_modules/istanbul/lib/cli.js --include-all-sources cover ./node_modules/mocha/bin/_mocha && istanbul check-coverage --statement 100 --branch 100 --function 100",
    "coveralls": "istanbul cover _mocha test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/achingbrain/node-wantsit.git"
  },
  "dependencies": {
    "async": "^1.4.0",
    "check-types": "^3.1.1",
    "toarray": "^0.0",
    "underscore.string": "^3.0.3"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "coveralls": "^2.8",
    "istanbul": "^0.3.0",
    "mocha": "^2.0",
    "pre-commit": "^1.0.6",
    "proxyquire": "^1.0",
    "sinon": "^1.8",
    "standard": "^4.5.4",
    "testsuite": "^1.0"
  },
  "pre-commit": [
    "lint",
    "test:coverage"
  ],
  "standard": {
    "global": [
      "describe",
      "it",
      "expect"
    ]
  }
}
