    "underscore.string": "^3.0.3"
  },
  "devDependencies": {
    "chai": "^2.1.2",
    "coveralls": "^2.8",
    "istanbul": "^0.3.0",
    "mocha": "^2.0",
    "pre-commit": "^1.0.6",
    "proxyquire": "^1.0",
    "sinon": "^1.8",
    "testsuite": "^1.0"
  },
  "jshintConfig": {
    "asi": true,
